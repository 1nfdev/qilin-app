{
    "parser"    : "babel-eslint",
    "extends"   : [ "eslint:recommended", "plugin:react/recommended" ],
    "plugins"   : [ "react" ],

    "env" : {
        "node"                  : true,
        "browser"               : true,
        "shared-node-browser"   : true,
        "es6"                   : true
    },

    "parserOptions" : {
        "ecmaVersion"   : 6,
        "sourceType"    : "module",
        "ecmaFeatures"  : {
            "jsx"                           : true,
            "experimentalObjectRestSpread"  : true
        }
    },

    "rules" : {
        "no-console"                    : [ 1 ],
        "no-debugger"                   : [ 1 ],
        "no-dupe-args"                  : [ 2 ],
        "no-dupe-keys"                  : [ 2 ],
        "no-duplicate-case"             : [ 2 ],
        "no-empty-character-class"      : [ 1 ],
        "no-empty"                      : [ 1, { "allowEmptyCatch" : true } ],
        "no-ex-assign"                  : [ 2 ],
        "no-extra-boolean-cast"         : [ 0 ],
        "no-extra-semi"                 : [ 1 ],
        "no-func-assign"                : [ 2 ],
        "no-inner-declarations"         : [ 1 ],
        "no-invalid-regexp"             : [ 2 ],
        "no-irregular-whitespace"       : [ 1 ],
        "no-obj-calls"                  : [ 2 ],
        "no-prototype-builtins"         : [ 0 ],
        "no-regex-spaces"               : [ 2 ],
        "no-sparse-arrays"              : [ 1 ],
        "no-template-curly-in-string"   : [ 2 ],
        "no-unexpected-multiline"       : [ 2 ],
        "no-unreachable"                : [ 1 ],
        "no-unsafe-finally"             : [ 1 ],
        "no-unsafe-negation"            : [ 1 ],
        "use-isnan"                     : [ 0 ],
        "valid-jsdoc"                   : [ 0 ],
        "valid-typeof"                  : [ 1 ],

        "accessor-pairs"                : [ 0 ],
        "array-callback-return"         : [ 0 ],
        "block-scoped-var"              : [ 2 ],
        "class-methods-use-this"        : [ 0 ],
        "complexity"                    : [ 0 ],
        "consistent-return"             : [ 0 ],
        "curly"                         : [ 1, "multi-or-nest", "consistent" ],
        "default-case"                  : [ 0 ],
        "dot-location"                  : [ 0 ],
        "dot-notation"                  : [ 1 ],
        "eqeqeq"                        : [ 1 ],
        "guard-for-in"                  : [ 0 ],
        "no-alert"                      : [ 2 ],
        "no-caller"                     : [ 2 ],
        "no-case-declarations"          : [ 1 ],
        "no-div-regex"                  : [ 0 ],
        "no-else-return"                : [ 0 ],
        "no-empty-function"             : [ 2 ],
        "no-empty-pattern"              : [ 1 ],
        "no-eq-null"                    : [ 2 ],
        "no-eval"                       : [ 2 ],
        "no-extend-native"              : [ 2 ],
        "no-extra-bind"                 : [ 1 ],
        "no-extra-label"                : [ 0 ],
        "no-fallthrough"                : [ 2 ],
        "no-floating-decimal"           : [ 0 ],
        "no-global-assign"              : [ 2 ],
        "no-implicit-coercion"          : [ 1 ],
        "no-implicit-globals"           : [ 0 ],
        "no-implied-eval"               : [ 2 ],
        "no-invalid-this"               : [ 0 ],
        "no-iterator"                   : [ 1 ],
        "no-labels"                     : [ 2 ],
        "no-lone-blocks"                : [ 2 ],
        "no-loop-func"                  : [ 2 ],
        "no-magic-numbers"              : [ 0 ],
        "no-multi-spaces"               : [ 0 ],
        "no-multi-str"                  : [ 0 ],
        "no-new-func"                   : [ 2 ],
        "no-new-wrappers"               : [ 0 ],
        "no-new"                        : [ 1 ],
        "no-octal-escape"               : [ 2 ],
        "no-octal"                      : [ 2 ],
        "no-param-reassign"             : [ 1 ],
        "no-proto"                      : [ 1 ],
        "no-redeclare"                  : [ 2 ],
        "no-return-assign"              : [ 1 ],
        "no-script-url"                 : [ 2 ],
        "no-self-assign"                : [ 2 ],
        "no-self-compare"               : [ 2 ],
        "no-sequences"                  : [ 0 ],
        "no-throw-literal"              : [ 2 ],
        "no-unmodified-loop-condition"  : [ 0 ],
        "no-unused-expressions"         : [ 0 ],
        "no-unused-labels"              : [ 2 ],
        "no-useless-call"               : [ 0 ],
        "no-useless-concat"             : [ 2 ],
        "no-useless-escape"             : [ 0 ],
        "no-void"                       : [ 2 ],
        "no-warning-comments"           : [ 1 ],
        "no-with"                       : [ 2 ],
        "radix"                         : [ 1 ],
        "vars-on-top"                   : [ 1 ],
        "wrap-iife"                     : [ 0 ],
        "yoda"                          : [ 1 ],

        "strict"                        : [ 0 ],

        "init-declarations"             : [ 0 ],
        "no-catch-shadow"               : [ 2 ],
        "no-delete-var"                 : [ 2 ],
        "no-label-var"                  : [ 2 ],
        "no-restricted-globals"         : [ 0 ],
        "no-shadow-restricted-names"    : [ 2 ],
        "no-shadow"                     : [ 1 ],
        "no-undef-init"                 : [ 2 ],
        "no-undef"                      : [ 0 ],
        "no-undefined"                  : [ 1 ],
        "no-unused-vars"                : [ 1 ],
        "no-use-before-define"          : [ 2 ],

        "callback-return"               : [ 1 ],
        "global-require"                : [ 0 ],
        "handle-callback-err"           : [ 0 ],
        "no-mixed-requires"             : [ 0 ],
        "no-new-require"                : [ 2 ],
        "no-path-concat"                : [ 1 ],
        "no-process-env"                : [ 0 ],
        "no-process-exit"               : [ 1 ],
        "no-restricted-modules"         : [ 0 ],
        "no-sync"                       : [ 1 ],

        "array-bracket-spacing"         : [ 2, "always" ],
        "block-spacing"                 : [ 2, "always" ],
        "brace-style"                   : [ 2, "1tbs" ],
        "camelcase"                     : [ 2, { "properties" : "always" } ],
        "comma-dangle"                  : [ 2, "always-multiline" ],
        "comma-spacing"                 : [ 2 ],
        "comma-style"                   : [ 2, "last" ],
        "computed-property-spacing"     : [ 2, "always" ],
        "consistent-this"               : [ 2, "self" ],
        "eol-last"                      : [ 2, "always" ],
        "func-call-spacing"             : [ 2, "never" ],
        "func-names"                    : [ 0 ],
        "func-style"                    : [ 0 ],
        "id-blacklist"                  : [ 0 ],
        "id-length"                     : [ 0 ],
        "id-match"                      : [ 0 ],
        "indent"                        : [ 2, 4, { "SwitchCase" : 1 } ],
        "jsx-quotes"                    : [ 2, "prefer-double" ],
        "key-spacing"                   : [ 2, {
            "singleLine" : { "beforeColon" : true, "afterColon" : true },
            "multiLine"  : { "beforeColon" : true, "afterColon" : true, "align" : "colon" }
        } ],
        "keyword-spacing"               : [ 2, { "before" : true, "after" : true } ],
        "line-comment-position"         : [ 1, { "position" : "above" } ],
        "linebreak-style"               : [ 0 ],
        "lines-around-comment"          : [ 0 ],
        "lines-around-directive"        : [ 1, { "before" : "never", "after" : "always" } ],
        "max-depth"                     : [ 2, { "max": 4 } ],
        "max-len"                       : [ 1, 120 ],
        "max-lines"                     : [ 0 ],
        "max-nested-callbacks"          : [ 1, 4 ],
        "max-params"                    : [ 0 ],
        "max-statements-per-line"       : [ 2 ],
        "max-statements"                : [ 0 ],
        "multiline-ternary"             : [ 0 ],
        "new-cap"                       : [ 1 ],
        "new-parens"                    : [ 0 ],
        "newline-after-var"             : [ 1, "always" ],
        "newline-before-return"         : [ 2 ],
        "newline-per-chained-call"      : [ 1, { "ignoreChainWithDepth" : 3 } ],
        "no-array-constructor"          : [ 2 ],
        "no-bitwise"                    : [ 0 ],
        "no-continue"                   : [ 0 ],
        "no-inline-comments"            : [ 0 ],
        "no-lonely-if"                  : [ 2 ],
        "no-mixed-operators"            : [ 1 ],
        "no-mixed-spaces-and-tabs"      : [ 2 ],
        "no-multiple-empty-lines"       : [ 2, { "max" : 2 } ],
        "no-negated-condition"          : [ 1 ],
        "no-nested-ternary"             : [ 1 ],
        "no-new-object"                 : [ 2 ],
        "no-plusplus"                   : [ 0 ],
        "no-restricted-syntax"          : [ 0 ],
        "no-tabs"                       : [ 2 ],
        "no-ternary"                    : [ 0 ],
        "no-trailing-spaces"            : [ 2 ],
        "no-underscore-dangle"          : [ 1 ],
        "no-unneeded-ternary"           : [ 2 ],
        "no-whitespace-before-property" : [ 2 ],
        "object-curly-newline"          : [ 0 ],
        "object-curly-spacing"          : [ 2, "always" ],
        "object-property-newline"       : [ 0 ],
        "one-var-declaration-per-line"  : [ 0 ],
        "one-var"                       : [ 2, "never" ],
        "operator-assignment"           : [ 1, "always" ],
        "operator-linebreak"            : [ 1, "before" ],
        "padded-blocks"                 : [ 2, "never" ],
        "quote-props"                   : [ 1, "as-needed" ],
        "quotes"                        : [ 2, "double" ],
        "require-jsdoc"                 : [ 0 ],
        "semi-spacing"                  : [ 2, { "before" : false, "after" : false } ],
        "semi"                          : [ 2, "always" ],
        "sort-keys"                     : [ 0 ],
        "sort-vars"                     : [ 0 ],
        "space-before-blocks"           : [ 2, "always" ],
        "space-before-function-paren"   : [ 2, "never" ],
        "space-in-parens"               : [ 2, "always" ],
        "space-infix-ops"               : [ 2 ],
        "space-unary-ops"               : [ 0 ],
        "spaced-comment"                : [ 1, "always" ],
        "unicode-bom"                   : [ 2, "never" ],
        "wrap-regex"                    : [ 0 ],

        "arrow-body-style"              : [ 2, "as-needed" ],
        "arrow-parens"                  : [ 2, "as-needed" ],
        "arrow-spacing"                 : [ 2, { "before" : true, "after" : true } ],
        "constructor-super"             : [ 1 ],
        "generator-star-spacing"        : [ 1, { "before" : false, "after" : true } ],
        "no-class-assign"               : [ 2 ],
        "no-confusing-arrow"            : [ 1 ],
        "no-const-assign"               : [ 2 ],
        "no-dupe-class-members"         : [ 2 ],
        "no-duplicate-imports"          : [ 1 ],
        "no-new-symbol"                 : [ 1 ],
        "no-restricted-imports"         : [ 0 ],
        "no-this-before-super"          : [ 2 ],
        "no-useless-computed-key"       : [ 1 ],
        "no-useless-constructor"        : [ 1 ],
        "no-useless-rename"             : [ 1 ],
        "no-var"                        : [ 1 ],
        "object-shorthand"              : [ 1 ],
        "prefer-arrow-callback"         : [ 1 ],
        "prefer-const"                  : [ 1 ],
        "prefer-numeric-literals"       : [ 0 ],
        "prefer-reflect"                : [ 0 ],
        "prefer-rest-params"            : [ 1 ],
        "prefer-spread"                 : [ 1 ],
        "prefer-template"               : [ 2 ],
        "require-yield"                 : [ 1 ],
        "rest-spread-spacing"           : [ 2, "never" ],
        "sort-imports"                  : [ 0 ],
        "symbol-description"            : [ 0 ],
        "template-curly-spacing"        : [ 1, "never" ],
        "yield-star-spacing"            : [ 1, "before" ]
    }
 }
